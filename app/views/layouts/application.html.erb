<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= content_for?(:title) ? yield(:title) : "Which One Is Higher?" %></title>
    <meta name="description" content="<%= content_for?(:description) ? yield(:description) : "Which One Is Higher?" %>">
    <%= stylesheet_link_tag "application", media: "all" %>
    <%= google_maps_api %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <header>
      <%= render 'layouts/navigation' %>
    </header>
    <div class="container">
       <%= render 'layouts/messages' %>
       <%= yield %>
    </div>
    <footer>
      <%= render 'layouts/footer' %>
    </footer>
  </body>
</html>
